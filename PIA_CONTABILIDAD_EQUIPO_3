        "            [\"Unidades a vender\", precio_venta_zapatos_diccionario[\"ventas_planeadas_primer_semestre\"], precio_venta_zapatos_diccionario[\"ventas_planeadas_segundo_semestre\"], None],\n",
        "            [\"Precio de Venta\", precio_venta_zapatos_diccionario[\"precio_ventas_primer_semestre\"], precio_venta_zapatos_diccionario[\"precio_ventas_segundo_semestre\"], None],\n",
        "            [\"Importe de Venta\", importe_ventas_zapatos_1, importe_ventas_zapatos_2, importe_venta_zapatos_2016],\n",
        "            [None, None, None, None],\n",
        "            [\"PRODUCTO Tenis\", None, None, None],\n",
        "            [\"Unidades a vender\", precio_venta_tenis_diccionario[\"ventas_planeadas_primer_semestre\"], precio_venta_tenis_diccionario[\"ventas_planeadas_segundo_semestre\"], None],\n",
        "            [\"Precio de Venta\", precio_venta_tenis_diccionario[\"precio_ventas_primer_semestre\"], precio_venta_tenis_diccionario[\"precio_ventas_segundo_semestre\"], None],\n",
        "            [\"Importe de Venta\", importe_venta_1, importe_venta_2, importe_ventas_2016],\n",
        "            ['Total de Ventas por Semestre', importe_venta_total_1, importe_venta_total_2, importe_ventas_total_2016]\n",
        "        ]\n",
        "\n",
        "        print(tabulate(presupuesto_de_ventas, headers=[\"\", \"1er.Semestre\", \"2do.Semestre\", \"2016\"], tablefmt=\"simple_grid\"))\n",
        "        \"\"\"\n",
        "        print(\"\\nProducto Chanclas: \")\n",
        "        print(f\"Importe Venta 1er. Semestre: {importe_venta_chanclas_1:,.1f}\")\n",
        "        print(f\"Importe Venta 2do. Semestre: {importe_venta_chanclas_2:,.1f}\")\n",
        "        print(f\"Importe Venta 2016:  {importe_venta_chanclas_2016:,.1f}\")\n",
        "        print(\"\\nProducto Zapatos: \")\n",
        "        print(f\"Importe Venta 1er. Semestre: {importe_ventas_zapatos_1:,.1f}\")\n",
        "        print(f\"Importe Venta 2do. Semestre: {importe_ventas_zapatos_2:,.1f}\")\n",
        "        print(f\"Importe Venta 2016:  {importe_venta_zapatos_2016:,.1f}\")\n",
        "        print(\"\\nProducto Tenis: \")\n",
        "        print(f\"Importe Venta 1er. Semestre: {importe_ventas_tenis_1:,.1f}\")\n",
        "        print(f\"Importe Venta 2do. Semestre: {importe_ventas_tenis_2:,.1f}\")\n",
        "        print(f\"Importe Venta 2016:  {importe_ventas_tenis_2016:,.1f}\")\n",
        "        print(\"\\nTotal de Ventas por Semestre:\")\n",
        "        print(f\"1er. Semestre: {importe_venta_total_1:,.1f}\")\n",
        "        print(f\"2do. Semestre: {importe_venta_total_2:,.1f}\")\n",
        "        print(f\"2016: {importe_ventas_total_2016:,.1f}\\n\")\n",
        "        \"\"\"\n",
        "        print()\n",
        "        print(\"*\" * 120)\n",
        "        print(\"2. Determinación del saldo de Clientes y Flujo de Entradas\")\n",
        "\n",
        "        total_clientes_2016 = clientes + importe_ventas_total_2016\n",
        "        por_cobranza_2016 = importe_ventas_total_2016 * 0.8\n",
        "        total_por_cobranza = clientes + por_cobranza_2016\n",
        "        saldo_clientes_2016 = total_clientes_2016 - total_por_cobranza\n",
        "\n",
        "        Determinación_del_saldo_de_Clientes_y_Flujo_de_Entradas = [\n",
        "            ['Saldo de clientes 31-Dic-2015', None, formato_coma(clientes)],\n",
        "            ['Ventas 2016', None, formato_coma(importe_ventas_total_2016)],\n",
        "            ['Total de Clientes 2016', None, formato_coma(total_clientes_2016)],\n",
        "            [None, None, None, None],\n",
        "            ['Entradas de Efectivo:', None, None],\n",
        "            ['Por cobranza del 2015', formato_coma(clientes), None],\n",
        "            ['Por cobranza del 2016', formato_coma(por_cobranza_2016), None],\n",
        "            [None, None, formato_coma(total_por_cobranza)],\n",
        "            [None, None, None],\n",
        "            ['Saldo de Clientes del 2016', None, formato_coma(saldo_clientes_2016)]\n",
        "        ]\n",
        "\n",
        "        print(tabulate(Determinación_del_saldo_de_Clientes_y_Flujo_de_Entradas, headers=[\"Descripcion\", \"Importe\", \"Total\"], tablefmt=\"simple_grid\"))\n",
        "\n",
        "        print()\n",
        "        print(\"*\" * 120)\n",
        "        print(\"3. Presupuesto de Producción\")\n",
        "\n",
        "        chanclas_unidades_vender_1 = precio_venta_chanclas_diccionario[\"ventas_planeadas_primer_semestre\"]\n",
        "        chanclas_unidades_vender_2 = precio_venta_chanclas_diccionario[\"ventas_planeadas_segundo_semestre\"]\n",
        "        chanclas_unidades_vender_2016 = chanclas_unidades_vender_1 + chanclas_unidades_vender_2\n",
        "        chanclas_inventario_final_1 = inventario_inicial_primer_semestre_diccionario[\"producto_inicial_chanclas\"]\n",
        "        chanclas_inventario_final_2 = inventario_final_segundo_semestre_diccionario[\"producto_inicial_chanclas\"]\n",
        "        chanclas_inventario_final_2016 = inventario_final_segundo_semestre_diccionario[\"producto_inicial_chanclas\"]\n",
        "        chanclas_total_unidades_1 = chanclas_unidades_vender_1 + chanclas_inventario_final_1\n",
        "        chanclas_total_unidades_2 = chanclas_unidades_vender_2 + chanclas_inventario_final_2\n",
        "        chanclas_total_unidades_2016 = chanclas_unidades_vender_2016 + chanclas_inventario_final_2016\n",
        "        chanclas_unidades_producir_1 = chanclas_total_unidades_1 - chanclas_inventario_final_1\n",
        "        chanclas_unidades_producir_2 = chanclas_total_unidades_2 - chanclas_inventario_final_1\n",
        "        chanclas_unidades_producir_2016 = chanclas_total_unidades_2016 - chanclas_inventario_final_1\n",
        "\n",
        "        zapatos_unidades_vender_1 = precio_venta_zapatos_diccionario[\"ventas_planeadas_primer_semestre\"]\n",
        "        zapatos_unidades_vender_2 = precio_venta_zapatos_diccionario[\"ventas_planeadas_segundo_semestre\"]\n",
        "        zapatos_unidades_vender_2016 = zapatos_unidades_vender_1 + zapatos_unidades_vender_2\n",
        "        zapatos_inventario_final_1 = inventario_inicial_primer_semestre_diccionario[\"producto_inicial_zapatos\"]\n",
        "        zapatos_inventario_final_2 = inventario_final_segundo_semestre_diccionario[\"producto_inicial_zapatos\"]\n",
        "        zapatos_inventario_final_2016 = inventario_final_segundo_semestre_diccionario[\"producto_inicial_zapatos\"]\n",
        "        zapatos_total_unidades_1 = zapatos_unidades_vender_1 + zapatos_inventario_final_1\n",
        "        zapatos_total_unidades_2 = zapatos_unidades_vender_2 + zapatos_inventario_final_2\n",
        "        zapatos_total_unidades_2016 = zapatos_unidades_vender_2016 + zapatos_inventario_final_2016\n",
        "        zapatos_unidades_producir_1 = zapatos_total_unidades_1 - zapatos_inventario_final_1\n",
        "        zapatos_unidades_producir_2 = zapatos_total_unidades_2 - zapatos_inventario_final_1\n",
        "        zapatos_unidades_producir_2016 = zapatos_total_unidades_2016 - zapatos_inventario_final_1\n",
        "\n",
        "        tenis_unidades_vender_1 = precio_venta_tenis_diccionario[\"ventas_planeadas_primer_semestre\"]\n",
        "        tenis_unidades_vender_2 = precio_venta_tenis_diccionario[\"ventas_planeadas_segundo_semestre\"]\n",
        "        tenis_unidades_vender_2016 = tenis_unidades_vender_1 + tenis_unidades_vender_2\n",
        "        tenis_inventario_final_1 = inventario_inicial_primer_semestre_diccionario[\"producto_inicial_tenis\"]\n",
        "        tenis_inventario_final_2 = inventario_final_segundo_semestre_diccionario[\"producto_inicial_tenis\"]\n",
        "        tenis_inventario_final_2016 = inventario_final_segundo_semestre_diccionario[\"producto_inicial_tenis\"]\n",
        "        tenis_total_unidades_1 = tenis_unidades_vender_1 + tenis_inventario_final_1\n",
        "        tenis_total_unidades_2 = tenis_unidades_vender_2 + tenis_inventario_final_2\n",
        "        tenis_total_unidades_2016 = tenis_unidades_vender_2016 + tenis_inventario_final_2016\n",
        "        tenis_unidades_producir_1 = tenis_total_unidades_1 - tenis_inventario_final_1\n",
        "        tenis_unidades_producir_2 = tenis_total_unidades_2 - tenis_inventario_final_1\n",
        "        tenis_unidades_producir_2016 = tenis_total_unidades_2016 - tenis_inventario_final_1\n",
        "\n",
        "\n",
        "        Presupuesto_de_Producción = [\n",
        "            ['PRODUCTO Chanclas', None, None, None],\n",
        "            ['Unidades a vender', chanclas_unidades_vender_1, chanclas_unidades_vender_2, chanclas_unidades_vender_2016],\n",
        "            ['(+) Inventario Final', chanclas_inventario_final_1, chanclas_inventario_final_2, chanclas_inventario_final_2016],\n",
        "            ['(=) Total de Unidades', chanclas_total_unidades_1, chanclas_total_unidades_2, chanclas_total_unidades_2016],\n",
        "            ['(-) Inventario Inicial', chanclas_inventario_final_1, chanclas_inventario_final_1, chanclas_inventario_final_1],\n",
        "            ['(=) Unidades a Producir', chanclas_unidades_producir_1, chanclas_unidades_producir_2, chanclas_unidades_producir_2016],\n",
        "            [None, None, None, None],\n",
        "            ['PRODUCTO Zapatos', None, None, None],\n",
        "            ['Unidades a vender', zapatos_unidades_vender_1, zapatos_unidades_vender_2, zapatos_unidades_vender_2016],\n",
        "            ['(+) Inventario Final', zapatos_inventario_final_1, zapatos_inventario_final_2, zapatos_inventario_final_2016],\n",
        "            ['(=) Total de Unidades', zapatos_total_unidades_1, zapatos_total_unidades_2, zapatos_total_unidades_2016],\n",
        "            ['(-) Inventario Inicial', zapatos_inventario_final_1, zapatos_inventario_final_1, zapatos_inventario_final_1],\n",
        "            ['(=) Unidades a Producir', zapatos_unidades_producir_1, zapatos_unidades_producir_2, zapatos_unidades_producir_2016],\n",
        "            [None, None, None, None],\n",
        "            ['PRODUCTO Tenis', None, None, None],\n",
        "            ['Unidades a vender', tenis_unidades_vender_1, tenis_unidades_vender_2, tenis_unidades_vender_2016],\n",
        "            ['(+) Inventario Final', tenis_inventario_final_1, tenis_inventario_final_2, tenis_inventario_final_2016],\n",
        "            ['(=) Total de Unidades', tenis_total_unidades_1, tenis_total_unidades_2, tenis_total_unidades_2016],\n",
        "            ['(-) Inventario Inicial', tenis_inventario_final_1, tenis_inventario_final_1, tenis_inventario_final_1],\n",
