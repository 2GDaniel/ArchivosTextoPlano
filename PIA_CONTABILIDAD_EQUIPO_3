        "        material_a_comprar_C_2 = total_materiales_C_2 - inventario_final_C_1\n",
        "        material_a_comprar_C_2016 = total_materiales_C_2016 - inventario_final_C_1\n",
        "        precio_compra_C_1 = costos_primer_semestre_diccionario[\"materia_prima_C_piezas\"]\n",
        "        precio_compra_C_2 = costos_segundo_semestre_diccionario[\"materia_prima_C_piezas\"]\n",
        "        total_material_pesos_C_1 = material_a_comprar_C_1 * precio_compra_C_1\n",
