        "            [None, None, None, None],\n",
        "            ['Material C', None, None, None],\n",
        "            ['Requerimiento de materiales', final_total_material_C_requerido_1, final_total_material_C_requerido_2, final_total_material_C_requerido_2016],\n",
        "            ['(+) Inventario Final', inventario_final_C_1, inventario_final_C_2, inventario_final_C_2016],\n",
        "            ['Total de materiales', total_materiales_C_1, total_materiales_C_2, total_materiales_C_2016],\n",
        "            ['(-) Inventario Inicial', inventario_final_C_1, inventario_final_C_1, inventario_final_C_1],\n",
        "            ['Material a comprar', material_a_comprar_C_1, material_a_comprar_C_2, material_a_comprar_C_2016],\n",
        "            ['Precio de Compra', precio_compra_C_1, precio_compra_C_2, None],\n",
        "            ['Total de Material A en $', formato_coma(total_material_pesos_C_1), formato_coma(total_material_pesos_C_2), formato_coma(total_material_pesos_C_2016)],\n",
        "            [None, None, None, None],\n",
        "            ['Compras totales:', compras_totales_1, compras_totales_2, compras_totales_2016]\n",
        "        ]\n",
        "\n",
        "        print(tabulate(Presupuesto_de_Compra_de_Materiales, headers=[\"\", \"1er.Semestre\", \"2do.Semestre\", \"Total 2016\"], tablefmt=\"simple_grid\"))\n",
        "\n",
        "        print()\n",
        "        print(\"*\" * 120)\n",
        "        print(\"6. Determinación del saldo de Proveedores y Flujo de Salidas\")\n",
        "\n",
        "        total_proveedores_2016 = proveedores + compras_totales_2016\n",
        "        por_proveedores_2016 = compras_totales_2016 * .5\n",
        "        total_salidas_2016 = proveedores + por_proveedores_2016\n",
        "        saldo_proveedores_2016 = total_proveedores_2016 - total_salidas_2016\n",
        "\n",
        "        Determinación_del_saldo_de_Proveedores_y_Flujo_de_Salidas = [\n",
        "            ['Saldo de Proveedores 30-Dic-2015', None, proveedores],\n",
        "            ['Compras 2016', None, formato_coma(compras_totales_2016)],\n",
        "            ['Total de Proveedores 2016', None, total_proveedores_2016],\n",
        "            [None, None, None],\n",
        "            ['Salidas de Efectivo:', None, None],\n",
        "            ['Por proveedores del 2015', proveedores, '100%'],\n",
        "            ['Por Proveedores del 2016', formato_coma(por_proveedores_2016), '50%'],\n",
        "            ['Total de Salidas 2016', None, formato_coma(total_salidas_2016)],\n",
        "            [None, None, None],\n",
        "            ['Saldo de Proveedores del 2016', None, saldo_proveedores_2016]\n",
        "        ]\n",
        "\n",
        "        print(tabulate(Determinación_del_saldo_de_Proveedores_y_Flujo_de_Salidas, headers=[\"Descripcion\", \"Importe\", \"Total\"], tablefmt=\"simple_grid\"))\n",
        "\n",
        "        print()\n",
        "        print(\"*\" * 120)\n",
        "        print(\"7. Presupuesto de Mano de Obra Directa\")\n",
        "\n",
        "        chanclas_horas_requeridas_unidad_1 = materiales_chanclas_diccionario[\"horas_mano_obra\"]\n",
        "        chanclas_horas_requeridas_unidad_2 = materiales_chanclas_diccionario[\"horas_mano_obra\"]\n",
        "        chanclas_horas_requeridas_unidad_2016 = materiales_chanclas_diccionario[\"horas_mano_obra\"]\n",
        "        chanclas_total_horas_requeridas_1 = chanclas_unidades_producir_1 * chanclas_horas_requeridas_unidad_1\n",
        "        chanclas_total_horas_requeridas_2 = chanclas_unidades_producir_2 * chanclas_horas_requeridas_unidad_2\n",
        "        chanclas_total_horas_requeridas_2016 = chanclas_unidades_producir_2016 * chanclas_horas_requeridas_unidad_2016\n",
        "        chanclas_importe_mod_1 = chanclas_total_horas_requeridas_1 * 20\n",
        "        chanclas_importe_mod_2 = chanclas_total_horas_requeridas_2 * 24\n",
        "        chanclas_importe_mod_2016 = chanclas_importe_mod_1 + chanclas_importe_mod_2\n",
        "\n",
        "        zapatos_horas_requeridas_unidad_1 = materiales_zapatos_diccionario[\"horas_mano_obra\"]\n",
        "        zapatos_horas_requeridas_unidad_2 = materiales_zapatos_diccionario[\"horas_mano_obra\"]\n",
        "        zapatos_horas_requeridas_unidad_2016 = materiales_zapatos_diccionario[\"horas_mano_obra\"]\n",
        "        zapatos_total_horas_requeridas_1 = zapatos_unidades_producir_1 * zapatos_horas_requeridas_unidad_1\n",
        "        zapatos_total_horas_requeridas_2 = zapatos_unidades_producir_2 * zapatos_horas_requeridas_unidad_2\n",
        "        zapatos_total_horas_requeridas_2016 = zapatos_unidades_producir_2016 * zapatos_horas_requeridas_unidad_2016\n",
        "        zapatos_importe_mod_1 = zapatos_total_horas_requeridas_1 * 20\n",
        "        zapatos_importe_mod_2 = zapatos_total_horas_requeridas_2 * 24\n",
        "        zapatos_importe_mod_2016 = zapatos_importe_mod_1 + zapatos_importe_mod_2\n",
        "\n",
        "        tenis_horas_requeridas_unidad_1 = materiales_tenis_diccionario[\"horas_mano_obra\"]\n",
        "        tenis_horas_requeridas_unidad_2 = materiales_tenis_diccionario[\"horas_mano_obra\"]\n",
        "        tenis_horas_requeridas_unidad_2016 = materiales_tenis_diccionario[\"horas_mano_obra\"]\n",
        "        tenis_total_horas_requeridas_1 = tenis_unidades_producir_1 * tenis_horas_requeridas_unidad_1\n",
        "        tenis_total_horas_requeridas_2 = tenis_unidades_producir_2 * tenis_horas_requeridas_unidad_2\n",
        "        tenis_total_horas_requeridas_2016 = tenis_unidades_producir_2016 * tenis_horas_requeridas_unidad_2016\n",
        "        tenis_importe_mod_1 = tenis_total_horas_requeridas_1 * 20\n",
        "        tenis_importe_mod_2 = tenis_total_horas_requeridas_2 * 24\n",
        "        tenis_importe_mod_2016 = tenis_importe_mod_1 + tenis_importe_mod_2\n",
        "\n",
        "        total_horas_requeridas_s1 = chanclas_total_horas_requeridas_1 + zapatos_total_horas_requeridas_1 + tenis_total_horas_requeridas_1\n",
        "        total_horas_requeridas_s2 = chanclas_total_horas_requeridas_2 + zapatos_total_horas_requeridas_2 + tenis_total_horas_requeridas_2\n",
        "        total_horas_requeridas_s2016 = chanclas_total_horas_requeridas_2016 + zapatos_total_horas_requeridas_2016 + tenis_total_horas_requeridas_2016\n",
        "\n",
        "        importe_mod_s1 = chanclas_importe_mod_1 + zapatos_importe_mod_1 + tenis_importe_mod_1\n",
        "        importe_mod_s2 = chanclas_importe_mod_2 + zapatos_importe_mod_2 + tenis_importe_mod_2\n",
        "        importe_mod_s2016 = chanclas_importe_mod_2016 + zapatos_importe_mod_2016 + tenis_importe_mod_2016\n",
        "\n",
        "        Presupuesto_de_Mano_de_Obra_Directa = [\n",
        "            ['PRODUCTO Chanclas', None, None, None],\n",
        "            ['Unidades a producir', chanclas_unidades_producir_1, chanclas_unidades_producir_2, chanclas_unidades_producir_2016],\n",
        "            ['Horas requeridas por unidad', chanclas_horas_requeridas_unidad_1, chanclas_horas_requeridas_unidad_2, chanclas_horas_requeridas_unidad_2016],\n",
        "            ['Total de horas requeridas', chanclas_total_horas_requeridas_1, chanclas_total_horas_requeridas_2, chanclas_total_horas_requeridas_2016],\n",
        "            ['Cuota por hora', 20, 24, None],\n",
        "            ['Importe de M.D.D', chanclas_importe_mod_1, chanclas_importe_mod_2, formato_coma(chanclas_importe_mod_2016)],\n",
        "            [None, None, None, None],\n",
        "            ['PRODUCTO Zapatos', None, None, None],\n",
        "            ['Unidades a producir', zapatos_unidades_producir_1, zapatos_unidades_producir_2, zapatos_unidades_producir_2016],\n",
        "            ['Horas requeridas por unidad', zapatos_horas_requeridas_unidad_1, zapatos_horas_requeridas_unidad_2, zapatos_horas_requeridas_unidad_2016],\n",
        "            ['Total de horas requeridas', zapatos_total_horas_requeridas_1, zapatos_total_horas_requeridas_2, zapatos_total_horas_requeridas_2016],\n",
        "            ['Cuota por hora', 20, 24, None],\n",
        "            ['Importe de M.D.D', zapatos_importe_mod_1, zapatos_importe_mod_2, formato_coma(zapatos_importe_mod_2016)],\n",
        "            [None, None, None, None],\n",
        "            ['PRODUCTO Tenis', None, None, None],\n",
        "            ['Unidades a producir', tenis_unidades_producir_1, tenis_unidades_producir_2, tenis_unidades_producir_2016],\n",
        "            ['Horas requeridas por unidad', tenis_horas_requeridas_unidad_1, tenis_horas_requeridas_unidad_2, tenis_horas_requeridas_unidad_2016],\n",
        "            ['Total de horas requeridas', tenis_total_horas_requeridas_1, tenis_total_horas_requeridas_2, tenis_total_horas_requeridas_2016],\n",
        "            ['Cuota por hora', 20, 24, None],\n",
        "            ['Importe de M.D.D', tenis_importe_mod_1, tenis_importe_mod_2, formato_coma(tenis_importe_mod_2016)],\n",
        "            [None, None, None, None],\n",
        "            ['Total de horas requeridas por semestre', total_horas_requeridas_s1, total_horas_requeridas_s2, total_horas_requeridas_s2016],\n",
        "            ['Total de M.O.D por semestre', formato_coma(importe_mod_s1), formato_coma(importe_mod_s2), formato_coma(importe_mod_s2016)]\n",
        "        ]\n",
        "\n",
        "        print(tabulate(Presupuesto_de_Mano_de_Obra_Directa, headers=[\"\", \"1er.Semestre\", \"2do.Semestre\", \"Total 2016\"], tablefmt=\"simple_grid\"))\n",
        "\n",
        "        print()\n",
        "        print(\"*\" * 120)\n",
        "        print(\"8. Presupuesto de Gastos Indirectos de Fabricación\")\n",
        "\n",
        "        depreciacion_1 = gastos_fabricacion_indirectos_diccionario[\"depreciacion_anual\"] / 2\n",
        "        depreciacion_2 = gastos_fabricacion_indirectos_diccionario[\"depreciacion_anual\"] / 2\n",
        "        depreciacion_2016 = gastos_fabricacion_indirectos_diccionario[\"depreciacion_anual\"]\n",
        "        seguros_1 = gastos_fabricacion_indirectos_diccionario[\"seguros_anuales\"] / 2\n",
        "        seguros_2 = gastos_fabricacion_indirectos_diccionario[\"seguros_anuales\"] / 2\n",
        "        seguros_2016 = gastos_fabricacion_indirectos_diccionario[\"seguros_anuales\"]\n",
        "        mantenimiento_1 = gastos_fabricacion_indirectos_diccionario[\"mantenimiento_primer_semestre\"]\n",
        "        mantenimiento_2 = gastos_fabricacion_indirectos_diccionario[\"mantenimiento_segundo_semestre\"]\n",
        "        mantenimiento_2016 = mantenimiento_1 + mantenimiento_2\n",
        "        energeticos_1 = gastos_fabricacion_indirectos_diccionario[\"energeticos_primer_semestre\"]\n",
        "        energeticos_2 = gastos_fabricacion_indirectos_diccionario[\"energeticos_segundo_semestre\"]\n",
        "        energeticos_2016 = energeticos_1 + energeticos_2\n",
        "        varios_1 = gastos_fabricacion_indirectos_diccionario[\"varios_anuales\"] / 2\n",
        "        varios_2 = gastos_fabricacion_indirectos_diccionario[\"varios_anuales\"] / 2\n",
        "        varios_2016 = varios_1 + varios_2\n",
        "        total_gif_1 = depreciacion_1 + seguros_1 + energeticos_1 + mantenimiento_1 + varios_1\n",
        "        total_gif_2 = depreciacion_2 + seguros_2 + energeticos_2 + mantenimiento_2 + varios_2\n",
        "        total_gif_2016 = depreciacion_2016 + seguros_2016 + energeticos_2016 + mantenimiento_2016 + varios_2016\n",
        "        costo_por_hora_gif = total_gif_2016 / total_horas_requeridas_s2016\n",
        "\n",
        "        Presupuesto_de_Gastos_Indirectos_de_Fabricación = [\n",
        "            ['Depreciacion', depreciacion_1, depreciacion_2, depreciacion_2016],\n",
        "            ['Seguros', seguros_1, seguros_2, seguros_2016],\n",
        "            ['Mantenimiento', mantenimiento_1, mantenimiento_2, mantenimiento_2016],\n",
        "            ['Energeticos', energeticos_1, energeticos_2, energeticos_2016],\n",
        "            ['Varios', varios_1, varios_2, varios_2016],\n",
        "            ['Total G.I.F por semestre', total_gif_1, total_gif_2, total_gif_2016],\n",
        "            [None, None, None, None],\n",
        "            ['Coeficiente para determinar el costo por hora de Gastos Indirectos de Fabricación', None, None, None],\n",
        "            ['Total de G.I.F', None, None, total_gif_2016],\n",
        "            ['(/) Total horas M.O.D Anual', None, None, total_horas_requeridas_s2016],\n",
        "            ['(=) Costo por Hora de G.I.F.', None, None, costo_por_hora_gif]\n",
        "        ]\n",
        "\n",
        "        print(tabulate(Presupuesto_de_Gastos_Indirectos_de_Fabricación, headers=[\"\", \"1er.Semestre\", \"2do.Semestre\", \"Total 2016\"], tablefmt=\"simple_grid\"))\n",
        "\n",
        "        print()\n",
        "        print(\"*\" * 120)\n",
        "        print(\"9. Presupuesto de Gastos de Operación\")\n",
        "\n",
        "        depreciacion_gasto_op_1 = gasto_de_administracion_y_ventas_diccionario[\"depreciacion_anual\"] / 2\n",
        "        depreciacion_gasto_op_2 = gasto_de_administracion_y_ventas_diccionario[\"depreciacion_anual\"] / 2\n",
        "        depreciacion_gasto_op_2016 = gasto_de_administracion_y_ventas_diccionario[\"depreciacion_anual\"]\n",
        "        sueldos_y_salarios_1 = gasto_de_administracion_y_ventas_diccionario[\"sueldos_salarios_anuales\"] / 2\n",
        "        sueldos_y_salarios_2 = gasto_de_administracion_y_ventas_diccionario[\"sueldos_salarios_anuales\"] / 2\n",
        "        sueldos_y_salarios_2016 = gasto_de_administracion_y_ventas_diccionario[\"sueldos_salarios_anuales\"]\n",
        "        comisiones_1 = importe_venta_total_1 * .01\n",
        "        comisiones_2 = importe_venta_total_2 * .01\n",
        "        comisiones_2016 = importe_ventas_total_2016 * .01\n",
        "        varios_gasto_op_1 = gasto_de_administracion_y_ventas_diccionario[\"varios_primer_semestre\"]\n",
        "        varios_gasto_op_2 = gasto_de_administracion_y_ventas_diccionario[\"varios_segundo_semestre\"]\n",
        "        varios_gasto_op_2016 = varios_gasto_op_1 + varios_gasto_op_2\n",
        "        intereses_del_prestamo_1 = gasto_de_administracion_y_ventas_diccionario[\"intereses_prestamo_anuales\"] / 2\n",
        "        intereses_del_prestamo_2 = gasto_de_administracion_y_ventas_diccionario[\"intereses_prestamo_anuales\"] / 2\n",
        "        intereses_del_prestamo_2016 = intereses_del_prestamo_1 + intereses_del_prestamo_2\n",
        "\n",
        "        total_gastos_op_1 = depreciacion_gasto_op_1 + sueldos_y_salarios_1 + comisiones_1 + varios_gasto_op_1 + intereses_del_prestamo_1\n",
        "        total_gastos_op_2 = depreciacion_gasto_op_2 + sueldos_y_salarios_2 + comisiones_2 + varios_gasto_op_2 + intereses_del_prestamo_2\n",
        "        total_gastos_op_2016 = depreciacion_gasto_op_2016 + sueldos_y_salarios_2016 + comisiones_2016 + varios_gasto_op_2016 + intereses_del_prestamo_2016\n",
        "\n",
        "        Presupuesto_de_Gastos_de_Operación = [\n",
        "            ['Depreciacion', depreciacion_gasto_op_1, depreciacion_gasto_op_2, depreciacion_gasto_op_2016],\n",
        "            ['Sueldos y Salarios', sueldos_y_salarios_1, sueldos_y_salarios_2, sueldos_y_salarios_2016],\n",
        "            ['Comisiones', comisiones_1, comisiones_2, comisiones_2016],\n",
        "            ['Varios', varios_gasto_op_1, varios_gasto_op_2, varios_gasto_op_2016],\n",
        "            ['Intereses del Prestamo', intereses_del_prestamo_1, intereses_del_prestamo_2, intereses_del_prestamo_2016],\n",
        "            ['Total de Gastos de Operacion', total_gastos_op_1, total_gastos_op_2, total_gastos_op_2016]\n",
        "        ]\n",
        "\n",
        "        print(tabulate(Presupuesto_de_Gastos_de_Operación, headers=[\"\", \"1er.Semestre\", \"2do.Semestre\", \"Total 2016\"], tablefmt=\"simple_grid\"))\n",
        "\n",
        "        print()\n",
        "        print(\"*\" * 120)\n",
        "        print(\"10. Determinación del Costo Unitario de Productos Terminados\")\n",
        "\n",
        "        chanclas_costo_unitario_A = precio_compra_A_2 * chanclas_requerimiento_material_A_2\n",
        "        chanclas_costo_unitario_B = precio_compra_B_2 * chanclas_requerimiento_material_B_2\n",
        "        chanclas_costo_unitario_C = precio_compra_C_2 * chanclas_requerimiento_material_C_2\n",
        "        chanclas_mano_obra_cu = 24 * chanclas_horas_requeridas_unidad_2\n",
        "        chanclas_gif_cu = costo_por_hora_gif * chanclas_horas_requeridas_unidad_2\n",
        "        chanclas_costo_unitario_total = chanclas_costo_unitario_A + chanclas_costo_unitario_B + chanclas_costo_unitario_C + chanclas_mano_obra_cu + chanclas_gif_cu\n",
        "\n",
        "        zapatos_costo_unitario_A = precio_compra_A_2 * zapatos_requerimiento_material_A_2\n",
        "        zapatos_costo_unitario_B = precio_compra_B_2 * zapatos_requerimiento_material_B_2\n",
        "        zapatos_costo_unitario_C = precio_compra_C_2 * zapatos_requerimiento_material_C_2\n",
        "        zapatos_mano_obra_cu = 24 * zapatos_horas_requeridas_unidad_2\n",
        "        zapatos_gif_cu = costo_por_hora_gif * zapatos_horas_requeridas_unidad_2\n",
        "        zapatos_costo_unitario_total = (zapatos_costo_unitario_A + zapatos_costo_unitario_B +\n",
        "                                        zapatos_costo_unitario_C + zapatos_mano_obra_cu + zapatos_gif_cu)\n",
        "\n",
        "        tenis_costo_unitario_A = precio_compra_A_2 * tenis_requerimiento_material_A_2\n",
        "        tenis_costo_unitario_B = precio_compra_B_2 * tenis_requerimiento_material_B_2\n",
        "        tenis_costo_unitario_C = precio_compra_C_2 * tenis_requerimiento_material_C_2\n",
        "        tenis_mano_obra_cu = 24 * tenis_horas_requeridas_unidad_2\n",
        "        tenis_gif_cu = costo_por_hora_gif * tenis_horas_requeridas_unidad_2\n",
        "        tenis_costo_unitario_total = (tenis_costo_unitario_A + tenis_costo_unitario_B +\n",
        "                                    tenis_costo_unitario_C + tenis_mano_obra_cu + tenis_gif_cu)\n",
        "\n",
        "        Determinación_del_Costo_Unitario_de_Productos_Terminados = [\n",
        "            [None, None, 'PRODUCTO Chanclas', None],\n",
        "            ['Descripcion', 'Costo', 'Cantidad', 'Costo Unitario'],\n",
        "            ['Material A', precio_compra_A_2, chanclas_requerimiento_material_A_2, chanclas_costo_unitario_A],\n",
        "            ['Material B', precio_compra_B_2, chanclas_requerimiento_material_B_2, chanclas_costo_unitario_B],\n",
        "            ['Material C', precio_compra_C_2, chanclas_requerimiento_material_C_2, chanclas_costo_unitario_C],\n",
        "            ['Mano de Obra', 24, chanclas_horas_requeridas_unidad_2, chanclas_mano_obra_cu],\n",
        "            ['Gastos Indirectos de Fabricacion', costo_por_hora_gif, chanclas_horas_requeridas_unidad_2, chanclas_gif_cu],\n",
        "            ['Costo Unitario', None, None, chanclas_costo_unitario_total],\n",
        "\n",
        "            [None, None, 'PRODUCTO Zapatos', None],\n",
        "            ['Descripcion', 'Costo', 'Cantidad', 'Costo Unitario'],\n",
        "            ['Material A', precio_compra_A_2, zapatos_requerimiento_material_A_2, zapatos_costo_unitario_A],\n",
        "            ['Material B', precio_compra_B_2, zapatos_requerimiento_material_B_2, zapatos_costo_unitario_B],\n",
        "            ['Material C', precio_compra_C_2, zapatos_requerimiento_material_C_2, zapatos_costo_unitario_C],\n",
        "            ['Mano de Obra', 24, zapatos_horas_requeridas_unidad_2, zapatos_mano_obra_cu],\n",
        "            ['Gastos Indirectos de Fabricacion', costo_por_hora_gif, zapatos_horas_requeridas_unidad_2, zapatos_gif_cu],\n",
        "            ['Costo Unitario', None, None, zapatos_costo_unitario_total],\n",
        "\n",
        "            [None, None, 'PRODUCTO Tenis', None],\n",
        "            ['Descripcion', 'Costo', 'Cantidad', 'Costo Unitario'],\n",
        "            ['Material A', precio_compra_A_2, tenis_requerimiento_material_A_2, tenis_costo_unitario_A],\n",
        "            ['Material B', precio_compra_B_2, tenis_requerimiento_material_B_2, tenis_costo_unitario_B],\n",
        "            ['Material C', precio_compra_C_2, tenis_requerimiento_material_C_2, tenis_costo_unitario_C],\n",
        "            ['Mano de Obra', 24, tenis_horas_requeridas_unidad_2, tenis_mano_obra_cu],\n",
        "            ['Gastos Indirectos de Fabricacion', costo_por_hora_gif, tenis_horas_requeridas_unidad_2, tenis_gif_cu],\n",
        "            ['Costo Unitario', None, None, tenis_costo_unitario_total]\n",
        "        ]\n",
        "\n",
        "        print(tabulate(Determinación_del_Costo_Unitario_de_Productos_Terminados, tablefmt=\"simple_grid\"))\n",
        "\n",
        "        print()\n",
        "        print(\"*\" * 120)\n",
        "        print(\"11. Valuación de Inventarios Finales\")\n",
        "\n",
        "        material_A_costo_total = inventario_final_A_2 * precio_compra_A_2\n",
        "        material_B_costo_total = inventario_final_B_2 * precio_compra_B_2\n",
        "        material_C_costo_total = inventario_final_C_2 * precio_compra_C_2\n",
        "        inventario_final_materiales_ct = material_A_costo_total + material_B_costo_total + material_C_costo_total\n",
        "\n",
        "        chanclas_costo_total = chanclas_inventario_final_2 * chanclas_costo_unitario_total\n",
        "        zapatos_costo_total = zapatos_inventario_final_2 * zapatos_costo_unitario_total\n",
        "        tenis_costo_total = tenis_inventario_final_2 * tenis_costo_unitario_total\n",
        "        inventario_final_producto_terminado = chanclas_costo_total + zapatos_costo_total + tenis_costo_total\n",
        "\n",
        "        Valuación_de_Inventarios_Finales = [\n",
        "            ['Inventario Final de Materiales', None, None, None],\n",
        "            ['Descripcion', 'Utilidades', 'Costo Unitario', 'Costo Total'],\n",
        "            ['Material A', inventario_final_A_2, precio_compra_A_2, material_A_costo_total],\n",
        "            ['Material B', inventario_final_B_2, precio_compra_B_2, material_B_costo_total],\n",
        "            ['Material C', inventario_final_C_2, precio_compra_C_2, material_C_costo_total],\n",
        "            ['Inventario Final de Materiales', None, None, inventario_final_materiales_ct],\n",
        "            ['Inventario Final de Producto Terminado', None, None, None],\n",
        "            ['Descripcion', 'Utilidades', 'Costo Unitario', 'Costo Total'],\n",
        "            ['PRODUCTO Chanclas', chanclas_inventario_final_2, chanclas_costo_unitario_total, chanclas_costo_total],\n",
        "            ['PRODUCTO Zapatos', zapatos_inventario_final_2, zapatos_costo_unitario_total, zapatos_costo_total],\n",
        "            ['PRODUCTO Zapatos', tenis_inventario_final_2, tenis_costo_unitario_total, tenis_costo_total],\n",
        "            ['Inventario Final de Producto Terminado', None, None, inventario_final_producto_terminado]\n",
        "        ]\n",
        "\n",
        "        print(tabulate(Valuación_de_Inventarios_Finales, tablefmt=\"simple_grid\"))\n",
        "\n",
        "        print(\"II. Presupuesto Financiero.\")\n",
        "        print(\"*\" * 120)\n",
        "        print(\"Estado de Costo de Producción y Ventas\")\n",
        "        print(\"Presupuesto del 1 de Enero al 31 de Diciembre del 2016\")\n",
        "\n",
        "        material_disponible = inventario_materiales + compras_totales_2016\n",
        "        materiales_utilizados = material_disponible - inventario_final_materiales_ct\n",
        "        costos_de_produccion = materiales_utilizados + importe_mod_s2016 + total_gif_2016\n",
        "        total_produccion_disponible = costos_de_produccion + inventario_producto_terminado\n",
        "        costo_de_ventas = total_produccion_disponible - inventario_final_producto_terminado\n",
        "\n",
        "        Estado_de_Costo_de_Producción_y_Ventas = [\n",
        "            ['Saldo Inicial de Materiales', None, inventario_materiales],\n",
        "            ['(+) Compras de Materiales', None, formato_coma(compras_totales_2016)],\n",
        "            ['(=) Material Disponible', None, formato_coma(material_disponible)],\n",
        "            ['(-) Inventario Final de Materiales', None, inventario_final_materiales_ct],\n",
        "            ['(=) Materiales Utilizados', None, formato_coma(materiales_utilizados)],\n",
        "            ['(+) Mano de Obra Directa', None, importe_mod_s2016],\n",
        "            ['(+) Gastos de Fabricacion Indirectos', None, total_gif_2016],\n",
        "            ['(=) Costo de Produccion', None, costos_de_produccion],\n",
        "            ['(+) Inventario Inicial de Productos Terminados', None, inventario_producto_terminado],\n",
        "            ['(=) Total de Produccion Disponible', None, total_produccion_disponible],\n",
        "            ['(-) Inventario Final de Productos Terminaods', None, inventario_final_producto_terminado],\n",
        "            ['(=) Costo de Ventas', None, formato_coma(costo_de_ventas)]\n",
        "        ]\n",
        "\n",
        "        print(tabulate(Estado_de_Costo_de_Producción_y_Ventas, tablefmt=\"simple_grid\"))\n",
        "\n",
        "        print(\"*\" * 120)\n",
        "        print(\"Estado de Resultados\")\n",
        "        print(\"Presupuesto del 1 de Enero al 31 de Diciembre del 2016\")\n",
        "\n",
        "        Utilidad_bruta = importe_ventas_total_2016 - costo_de_ventas\n",
        "        utilidad_de_operacion = Utilidad_bruta - total_gastos_op_2016\n",
        "        isr = utilidad_de_operacion * .3\n",
        "        ptu = utilidad_de_operacion * .1\n",
        "        utilidad_neta = utilidad_de_operacion - isr - ptu\n",
        "\n",
        "        Estado_de_Resultados = [\n",
        "            ['Ventas', formato_coma(importe_ventas_total_2016)],\n",
        "            ['(-) Costo de Ventas', formato_coma(costo_de_ventas)],\n",
        "            ['(=) Utilidad Bruta', formato_coma(Utilidad_bruta)],\n",
        "            ['(-) Gastos de Operacion', formato_coma(total_gastos_op_2016)],\n",
        "            ['(=) Utilidad de Operacion', formato_coma(utilidad_de_operacion)],\n",
        "            ['(-) ISR', formato_coma(isr)],\n",
        "            ['(-) PTU', formato_coma(ptu)],\n",
        "            ['(=) Utilidad Neta', formato_coma(utilidad_neta)]\n",
        "        ]\n",
        "\n",
        "        print(tabulate(Estado_de_Resultados, tablefmt=\"simple_grid\"))\n",
        "\n",
        "        print(\"*\" * 120)\n",
        "        print(\"Estado de Flujo de Efectivo\")\n",
        "        print(\"Presupuesto del 1 de Enero al 31 de Diciembre del 2016\")\n",
        "\n",
        "        total_entradas = por_cobranza_2016 + clientes\n",
        "        efectivo_disponible = efectivo + total_entradas\n",
        "        pago_gastos_indirectos_fabricacion = total_gif_2016 - depreciacion_2016\n",
        "        pago_gastos_de_operacion = total_gastos_op_2016 - depreciacion_gasto_op_2016\n",
        "        total_salidas = por_proveedores_2016 + proveedores + importe_mod_s2016 + pago_gastos_indirectos_fabricacion + pago_gastos_de_operacion + 120_000 + ISR_por_pagar + isr\n",
        "        flujo_efectivo_actual = efectivo_disponible - total_salidas\n",
        "\n",
        "        Estado_de_Flujo_de_Efectivo = [\n",
        "            ['Saldo Inicial de Efectivo', None, formato_coma(efectivo)],\n",
        "            ['Entradas:', None, None],\n",
        "            ['Cobranza 2016', formato_coma(por_cobranza_2016), None],\n",
        "            ['Cobranza 2015', formato_coma(clientes), None],\n",
        "            ['Total de Entradas', None, formato_coma(total_entradas)],\n",
        "            ['Efectivo Disponible', None, formato_coma(efectivo_disponible)],\n",
        "            ['Salidas:', None, None],\n",
        "            ['Proveedores 2016', formato_coma(por_proveedores_2016), None],\n",
        "            ['Proveedores 2015', formato_coma(proveedores), None],\n",
        "            ['Pago Mano de Obra Directa', formato_coma(importe_mod_s2016), None],\n",
        "            ['Pago Gastos Indirectos de Fabricación', formato_coma(pago_gastos_indirectos_fabricacion), None],\n",
        "            ['Pago de Gastos de Operación', formato_coma(pago_gastos_de_operacion), None],\n",
        "            ['Compra de Activo Fijo (Maquinaria)', 120_000, None],\n",
        "            ['Pago ISR 2015', formato_coma(ISR_por_pagar), None],\n",
        "            ['Pago ISR 2016', formato_coma(isr), None],\n",
        "            ['Total de Salidas', None, formato_coma(total_salidas)],\n",
        "            ['Flujo de Efectivo Actual', None, formato_coma(flujo_efectivo_actual)]\n",
        "        ]\n",
        "\n",
        "        print(tabulate(Estado_de_Flujo_de_Efectivo, tablefmt=\"simple_grid\"))\n",
        "\n",
        "        print(\"*\" * 120)\n",
        "        print(\"Balance General\")\n",
        "        print(\"Presupuesto del 1 de Enero al 31 de Diciembre del 2016\")\n",
        "\n",
        "        total_activos_circulantes = flujo_efectivo_actual + saldo_clientes_2016 + deudores_diversos + funcionarios_empleados + inventario_final_materiales_ct + inventario_final_producto_terminado\n",
        "        total_activos_NO_circulantes = terreno + (planta_equipo + 120_000) - (depreciacion_acumulada+depreciacion_2016+depreciacion_gasto_op_2016)\n",
        "        activo_total = total_activos_circulantes + total_activos_NO_circulantes\n",
        "\n",
        "        pasivos_corto_plazo = saldo_proveedores_2016 + documentos_por_pagar + ptu\n",
        "        pasivo_total = pasivos_corto_plazo + prestamos_bancarios\n",
        "\n",
        "        total_capital_contable = capital_contribuido + capital_ganado + utilidad_neta\n",
        "        suma_pasivo_capital = pasivo_total + total_capital_contable\n",
        "\n",
        "        balance_general = [\n",
        "            ['Activo', None, None],\n",
        "            ['Circulante', None, None],\n",
        "            ['Efectivo', flujo_efectivo_actual, None],\n",
        "            ['Clientes', saldo_clientes_2016, None],\n",
        "            ['Deudores Diversos', deudores_diversos, None],\n",
        "            ['Funcionarios y Empleados', funcionarios_empleados, None],\n",
        "            ['Inventario de Materiales', inventario_final_materiales_ct, None],\n",
        "            ['Inventario de Producto Terminado', inventario_final_producto_terminado, None],\n",
        "            ['Total Activos Circulantes:', None, formato_coma(total_activos_circulantes)],\n",
        "            [None, None, None,],\n",
        "            ['No circulante', None, None],\n",
        "            ['Terreno', terreno, None],\n",
        "            ['Planta y Equipo', formato_coma((planta_equipo + 120_000)), None],\n",
        "            ['Depreciacion Acumulada', depreciacion_acumulada+depreciacion_2016+depreciacion_gasto_op_2016, None],\n",
        "            ['Total Activos No Circulante', None, total_activos_NO_circulantes],\n",
        "            [None, None, None],\n",
        "            ['Activo total', None, formato_coma(activo_total)],\n",
        "            [None, None, None],\n",
        "            ['Pasivo', None, None],\n",
        "            ['Corto Plazo', None, None],\n",
        "            ['Proveedores', formato_coma(saldo_proveedores_2016), None],\n",
        "            ['Documentos por pagar', formato_coma(documentos_por_pagar), None],\n",
        "            ['ISR por Pagar', None, None],\n",
        "            ['PTU por Pagar', formato_coma(ptu), None],\n",
        "            ['Total de Pasivo Corto Plazo:', None, formato_coma(pasivos_corto_plazo)],\n",
        "            [None, None, None],\n",
        "            ['Largo Plazo', None, None],\n",
        "            ['Prestamos bancarios', formato_coma(prestamos_bancarios), None],\n",
        "            ['Total de Pasivo Largo Plazo:', None, formato_coma(prestamos_bancarios)],\n",
        "            [None, None, None],\n",
        "            ['PASIVO TOTAL', None, formato_coma(pasivo_total)],\n",
        "            [None, None, None],\n",
        "            ['Capital Contable', None, None],\n",
        "            ['Capital Aportado', formato_coma(capital_contribuido), None],\n",
        "            ['Capital Ganado', formato_coma(capital_ganado), None],\n",
        "            ['Utilidad del ejercicio', formato_coma(utilidad_neta), None],\n",
        "            ['Total de Capital Contable', None, formato_coma(total_capital_contable)],\n",
        "            [None, None, None],\n",
        "            ['SUMA DE PASIVO Y CAPITAL', None, formato_coma(suma_pasivo_capital)]\n",
        "        ]\n",
        "\n",
        "        print(tabulate(balance_general, tablefmt=\"simple_grid\"))"
      ]
    }
