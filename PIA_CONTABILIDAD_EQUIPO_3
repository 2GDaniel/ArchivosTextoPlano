        "inventario_inicial_primer_semestre = [0, 0, 0, 0, 0, 0]\n",
        "\n",
        "\n",
        "#inventario_final_segundo_semestre = [materia_prima_A_metros, materia_prima_B_metros, materia_prima_C_piezas, producto_inicial_chanclas, producto_inicial_zapatos, producto_inicial_tenis]\n",
        "inventario_final_segundo_semestre = [0, 0, 0, 0, 0, 0]\n",
        "\n",
        "\n",
        "#COSTOS\n",
        "#costos_primer_semestre = [materia_prima_A_metros, materia_prima_B_metros, materia_prima_C_piezas]\n",
        "costos_primer_semestre = [0, 0, 0]\n",
        "\n",
        "\n",
        "#costos_segundo_semestre = [materia_prima_A_metros, materia_prima_B_metros, materia_prima_C_piezas]\n",
        "costos_segundo_semestre = [0, 0, 0]\n",
        "\n",
        "\n",
        "\n",
        "#PRODUCTOS\n",
        "#precio_venta_chanclas = [precio_ventas_primer_semestre, precio_ventas_segundo_semestre, ventas_planeadas_primer_semestre, ventas_planeadas_segundo_semestre]\n",
        "precio_venta_chanclas = [0, 0, 0, 0]\n",
        "\n",
        "\n",
        "#precio_venta_zapatos = [precio_ventas_primer_semestre, precio_ventas_segundo_semestre, ventas_planeadas_primer_semestre, ventas_planeadas_segundo_semestre]\n",
        "precio_venta_zapatos = [0, 0, 0, 0]\n",
        "\n",
        "\n",
        "#precio_venta_tenis = [precio_ventas_primer_semestre, precio_ventas_segundo_semestre, ventas_planeadas_primer_semestre, ventas_planeadas_segundo_semestre]\n",
        "precio_venta_tenis = [0, 0, 0, 0]\n",
        "\n",
        "\n",
        "\n",
        "#GASTOS DE ADMINISTRACION Y VENTAS\n",
        "#gasto_de_administracion_y_ventas = [depreciacion_anual, sueldos_salarios_anuales, comisiones_ventas_proyectadas, varios_primer_semestre, varios_segundo_semestre, intereses_prestamo_anuales]\n",
        "gasto_de_administracion_y_ventas = [0, 0, 0, 0, 0, 0]\n",
        "\n",
        "\n",
        "\n",
        "#Gastos de Fabricación Indirectos\n",
        "#gastos_fabricacion_indirectos = [depreciacion_anual, seguros_anuales, mantenimiento_primer_semestre, mantenimiento_segundo_semestre, energeticos_primer_semestre, energeticos_segundo_semestre, varios_anuales]\n",
        "gastos_fabricacion_indirectos = [0, 0, 0, 0, 0, 0, 0]\n",
        "\n",
        "balance_ingresado = False\n",
        "materiales_ingresados = False\n",
        "informacion_inventario_ingresados = False\n",
        "productos_ingresados = False\n",
        "gastos_administracion_ingresados = False\n",
        "gastos_dabricacion_ingresados = False\n",
        "\n",
        "\n",
        "while True:\n",
        "    opcion_menu_general = input(\"\\n-- Menu principal --\\nBalance General: [1]\\nMateriales: [2]\\nInformacion de Inventarios: [3]\\nProductos: [4]\\nGastos de administracion y ventas: [5]\\nGastos de fabricacion indirectos: [6]\\nDatos Adicionales: [7]\\n\\nREPORTES: [8]\\nIngrese una opcion: \")\n",
        "    if opcion_menu_general == \"1\":\n",
        "        while True:\n",
        "            if balance_ingresado:\n",
        "                print(\"\\n** El balance general ya fue ingresado. **\")\n",
        "                break\n",
        "\n",
        "            print(\"\\n-- Presupuesto maestro --\")\n",
        "            print(\"Balance general al 31 de Diciembre del 2015\")\n",
        "\n",
        "            while True:\n",
        "                opcion_menu_balance = input(\"\\nActivos: [1]\\nPasivos: [2]\\nCapital Contable: [3]\\nSalir de Balance General: [0]\\nIngrese la opcion necesaria: \")\n",
        "\n",
        "                ''' ACTIVOS '''\n",
        "                if opcion_menu_balance == \"1\":\n",
        "                    print(\"\\n-- Activos --\")\n",
        "                    print(\"Activos circulantes\")\n",
        "\n",
        "                    efectivo = float(input(\"Ingrese la cantidad de efectivo: \"))\n",
        "                    clientes = float(input(\"Ingrese la cantidad de efectivo en clientes: \"))\n",
        "                    deudores_diversos = float(input(\"Ingrese la cantidad de efectivo en deudores diversos: \"))\n",
        "                    funcionarios_empleados = float(input(\"Ingrese la cantidad de efectivo en funcionarios y empleados: \"))\n",
        "                    inventario_materiales = float(input(\"Ingrese la cantidad en efectivo de inventario de materiales: \"))\n",
        "                    inventario_producto_terminado = float(input(\"Ingrese la cantidad en efectivo de inventario de producto terminado: \"))\n",
        "\n",
        "                    total_circulante = efectivo + clientes + deudores_diversos + funcionarios_empleados + inventario_materiales + inventario_producto_terminado\n",
        "\n",
        "                    print(f\"Total activo circulante: {total_circulante}\")\n",
        "\n",
        "                    print(\"\\nActivo no circulante\")\n",
        "                    terreno = float(input(\"Ingrese la cantidad de efectivo en terreno: \"))\n",
        "                    planta_equipo = float(input(\"Inrese la cantidad de efectivo en planta y equipo: \"))\n",
        "                    depreciacion_acumulada = float(input(\"Inrese la cantidad de efectivo en depreciacion acumulada: \"))\n",
        "                    activos_no_circulantes = [terreno, planta_equipo]\n",
        "\n",
        "                    total_no_circulante = terreno + planta_equipo - depreciacion_acumulada\n",
        "\n",
        "                    activo_total = total_circulante + total_no_circulante\n",
        "                    print(f\"\\nTotal de activo no circulante: {total_no_circulante}\")\n",
        "                    print(f\"\\nActivo total: {activo_total}\")\n",
        "\n",
        "                elif opcion_menu_balance == \"2\":\n",
        "                    print(\"\\n-- Pasivos --\")\n",
        "                    print(\"Pasivo a corto plazo\")\n",
        "\n",
        "                    proveedores = float(input(\"Ingrese la cantidad de efectivo en proveedores: \"))\n",
        "                    documentos_por_pagar = float(input(\"Ingrese la cantidad de efectivo en documentos por pagar: \"))\n",
        "                    ISR_por_pagar = float(input(\"Ingrese la cantidad de efectivo en ISR por pagar: \"))\n",
        "\n",
        "                    total_pasivo_corto_plazo = proveedores + documentos_por_pagar + ISR_por_pagar\n",
        "                    print(f\"El total de pasivo a corto plazo es: {total_pasivo_corto_plazo}\")\n",
        "\n",
        "                    print(\"\\nPasivo a largo plazo\")\n",
        "\n",
        "                    prestamos_bancarios = float(input(\"Ingrese la cantidad de prestamos bancarios: \"))\n",
        "                    total_pasivo_largo_plazo = prestamos_bancarios\n",
        "                    print(f\"El total del pasivo a largo plazo es: {total_pasivo_largo_plazo}\")\n",
        "\n",
        "                    pasivo_total = total_pasivo_corto_plazo + total_pasivo_largo_plazo\n",
        "                    print(f\"\\nEl pasivo total es: {pasivo_total}\")\n",
        "\n",
        "                elif opcion_menu_balance == \"3\":\n",
        "                    print(\"\\n-- Capital contable --\")\n",
        "\n",
        "                    capital_contribuido = float(input(\"Ingrese la cantidad de capital contribuido: \"))\n",
        "                    capital_ganado = float(input(\"Ingrese la cantidad de capital ganado: \"))\n",
        "\n",
        "                    capital_contable = capital_contribuido + capital_ganado\n",
        "\n",
        "                    print(f\"\\nComprobacion:\\nTotal activos: {activo_total}\\nTotal pasivos + Capital contable: {pasivo_total + capital_contable}\")\n",
        "\n",
        "                    balance_ingresado = True\n",
        "                elif opcion_menu_balance == \"0\":\n",
        "                    break\n",
        "\n",
        "            break\n",
        "\n",
        "    elif opcion_menu_general == \"2\":\n",
        "        while True:\n",
        "            print(\"\\n-- Materiales. --\")\n",
        "            print(\"*La hora de mano de obra directa costara $20 el primer semestre y 24 pesos el segundo semestre.\")\n",
        "            print(\"Los gastos indirectos de fabricacion se aplican con base en hora de mano de obra.*\\n\")\n",
        "\n",
        "            opcion_producto = input(\"Producto chanclas: [1]\\nProducto Zapatos: [2]\\nProducto tenis[3]\\nSalir de materiales: [0]\\nIngrese una opcion: \")\n",
        "            if opcion_producto == \"1\":\n",
        "                print(\"\\nMateriales para Chanclas.\")\n",
        "                materia_prima_A_metros = float(input(\"Ingrese la materia prima A en metros: \"))\n",
        "                materia_prima_B_metros = float(input(\"Ingrese la materia prima B en metros: \"))\n",
        "                materia_prima_C_piezas = float(input(\"Ingrese la materia prima C piezas: \"))\n",
        "                horas_mano_obra = float(input(\"Ingrese la mano de obra en horas: \"))\n",
        "\n",
        "                materiales_chanclas = [materia_prima_A_metros, materia_prima_B_metros, materia_prima_C_piezas, horas_mano_obra]\n",
        "                materiales_chanclas_diccionario = {\n",
        "                    \"materia_prima_A_metros\":materiales_chanclas[0],\n",
        "                    \"materia_prima_B_metros\":materiales_chanclas[1],\n",
        "                    \"materia_prima_C_piezas\":materiales_chanclas[2],\n",
        "                    \"horas_mano_obra\": materiales_chanclas[3]\n",
        "                }\n",
        "\n",
        "            elif opcion_producto == \"2\":\n",
        "                print(\"\\nMateriales para Zapatos.\")\n",
        "                materia_prima_A_metros = float(input(\"Ingrese la materia prima A en metros: \"))\n",
        "                materia_prima_B_metros = float(input(\"Ingrese la materia prima B en metros: \"))\n",
        "                materia_prima_C_piezas = float(input(\"Ingrese la materia prima C piezas: \"))\n",
        "                horas_mano_obra = float(input(\"Ingrese la mano de obra en horas: \"))\n",
        "\n",
        "                materiales_zapatos = [materia_prima_A_metros, materia_prima_B_metros, materia_prima_C_piezas, horas_mano_obra]\n",
        "                materiales_zapatos_diccionario = {\n",
        "                    \"materia_prima_A_metros\":materiales_zapatos[0],\n",
        "                    \"materia_prima_B_metros\":materiales_zapatos[1],\n",
        "                    \"materia_prima_C_piezas\":materiales_zapatos[2],\n",
        "                    \"horas_mano_obra\": materiales_zapatos[3]\n",
        "                }\n",
        "\n",
        "            elif opcion_producto == \"3\":\n",
        "                print(\"\\nMateriales para Tenis.\")\n",
        "                materia_prima_A_metros = float(input(\"Ingrese la materia prima A en metros: \"))\n",
        "                materia_prima_B_metros = float(input(\"Ingrese la materia prima B en metros: \"))\n",
        "                materia_prima_C_piezas = float(input(\"Ingrese la materia prima C piezas: \"))\n",
        "                horas_mano_obra = float(input(\"Ingrese la mano de obra en horas: \"))\n",
        "\n",
        "                materiales_tenis = [materia_prima_A_metros, materia_prima_B_metros, materia_prima_C_piezas, horas_mano_obra]\n",
        "                materiales_tenis_diccionario = {\n",
        "                    \"materia_prima_A_metros\":materiales_tenis[0],\n",
        "                    \"materia_prima_B_metros\":materiales_tenis[1],\n",
        "                    \"materia_prima_C_piezas\":materiales_tenis[2],\n",
        "                    \"horas_mano_obra\": materiales_tenis[3]\n",
        "                }\n",
        "\n",
        "            elif opcion_producto == \"0\":\n",
        "                break\n",
        "    elif opcion_menu_general == \"3\":\n",
        "        while True:\n",
        "            print(\"\\n-- Informacion de inventarios. --\")\n",
        "            print(\"*Suponga que los inventarios iniciales son iguales al final del primer semestre. No hay inventario de articulos en proceso.*\")\n",
        "            opcion_inventario = input(\"\\nInventario Inicial Primer semestre: [1]\\nInventario final Segundo semestre: [2]\\nCosto primer semestre[3]\\nCosto segundo semestre: [4]\\nSalir de inventarios: [0]\\nIngrese una opcion: \")\n",
        "\n",
        "            if opcion_inventario == \"1\":\n",
        "                print(\"\\nInventario inicial de primer semestre.\")\n",
        "                materia_prima_A_metros = float(input(\"Ingrese la meteria prima A en metros: \"))\n",
        "                materia_prima_B_metros = float(input(\"Ingrese la meteria prima B en metros: \"))\n",
        "                materia_prima_C_piezas = float(input(\"Ingrese la meteria prima C piezas: \"))\n",
        "                producto_inicial_chanclas = float(input(\"Ingrese el inventario inicial de chanclas: \"))\n",
        "                producto_inicial_zapatos = float(input(\"Ingrese el inventario inicial de zapatos: \"))\n",
        "                producto_inicial_tenis = float(input(\"Ingrese el inventario inicial de tenis: \"))\n",
        "\n",
        "                inventario_inicial_primer_semestre = [materia_prima_A_metros, materia_prima_B_metros, materia_prima_C_piezas, producto_inicial_chanclas, producto_inicial_zapatos, producto_inicial_tenis]\n",
        "                inventario_inicial_primer_semestre_diccionario = {\n",
        "                    \"materia_prima_A_metros\":inventario_inicial_primer_semestre[0],\n",
        "                    \"materia_prima_B_metros\":inventario_inicial_primer_semestre[1],\n",
        "                    \"materia_prima_C_piezas\":inventario_inicial_primer_semestre[2],\n",
        "                    \"producto_inicial_chanclas\":inventario_inicial_primer_semestre[3],\n",
        "                    \"producto_inicial_zapatos\":inventario_inicial_primer_semestre[4],\n",
        "                    \"producto_inicial_tenis\":inventario_inicial_primer_semestre[5]\n",
        "                }\n",
        "\n",
        "\n",
        "            elif opcion_inventario == \"2\":\n",
        "                print(\"\\nInventario final de segundo semestre.\")\n",
        "                materia_prima_A_metros = float(input(\"Ingrese la meteria prima A en metros: \"))\n",
        "                materia_prima_B_metros = float(input(\"Ingrese la meteria prima B en metros: \"))\n",
        "                materia_prima_C_piezas = float(input(\"Ingrese la meteria prima C piezas: \"))\n",
        "                producto_inicial_chanclas = float(input(\"Ingrese el inventario inicial de chanclas: \"))\n",
        "                producto_inicial_zapatos = float(input(\"Ingrese el inventario inicial de zapatos: \"))\n",
        "                producto_inicial_tenis = float(input(\"Ingrese el inventario inicial de tenis: \"))\n",
        "\n",
        "                inventario_final_segundo_semestre = [materia_prima_A_metros, materia_prima_B_metros, materia_prima_C_piezas, producto_inicial_chanclas, producto_inicial_zapatos, producto_inicial_tenis]\n",
        "                inventario_final_segundo_semestre_diccionario = {\n",
        "                    \"materia_prima_A_metros\":inventario_final_segundo_semestre[0],\n",
        "                    \"materia_prima_B_metros\":inventario_final_segundo_semestre[1],\n",
        "                    \"materia_prima_C_piezas\":inventario_final_segundo_semestre[2],\n",
        "                    \"producto_inicial_chanclas\":inventario_final_segundo_semestre[3],\n",
        "                    \"producto_inicial_zapatos\":inventario_final_segundo_semestre[4],\n",
        "                    \"producto_inicial_tenis\":inventario_final_segundo_semestre[5]\n",
        "                }\n",
        "\n",
        "            elif opcion_inventario == \"3\":\n",
        "                print(\"\\nCosto primer semestre.\")\n",
        "                materia_prima_A_metros = float(input(\"Ingrese el costo de materia prima A en metros del primer semestre: \"))\n",
        "                materia_prima_B_metros = float(input(\"Ingrese el costo de materia prima B en metros del primer semestre: \"))\n",
        "                materia_prima_C_piezas = float(input(\"Ingrese el costo de materia prima C en metros del primer semestre: \"))\n",
        "\n",
        "                costos_primer_semestre = [materia_prima_A_metros, materia_prima_B_metros, materia_prima_C_piezas]\n",
        "                costos_primer_semestre_diccionario = {\n",
        "                    \"materia_prima_A_metros\":costos_primer_semestre[0],\n",
        "                    \"materia_prima_B_metros\":costos_primer_semestre[1],\n",
        "                    \"materia_prima_C_piezas\":costos_primer_semestre[2]\n",
        "                }\n",
        "\n",
        "            elif opcion_inventario == \"4\":\n",
        "                materia_prima_A_metros = float(input(\"Ingrese el costo de materia prima A en metros del segundo semestre: \"))\n",
        "                materia_prima_B_metros = float(input(\"Ingrese el costo de materia prima B en metros del segundo semestre: \"))\n",
        "                materia_prima_C_piezas = float(input(\"Ingrese el costo de materia prima C en metros del segundo semestre: \"))\n",
        "\n",
        "                costos_segundo_semestre = [materia_prima_A_metros, materia_prima_B_metros, materia_prima_C_piezas]\n",
        "                costos_segundo_semestre_diccionario = {\n",
        "                    \"materia_prima_A_metros\":costos_segundo_semestre[0],\n",
        "                    \"materia_prima_B_metros\":costos_segundo_semestre[1],\n",
        "                    \"materia_prima_C_piezas\":costos_segundo_semestre[2]\n",
        "                }\n",
        "\n",
        "            elif opcion_inventario == \"0\":\n",
        "                break\n",
        "\n",
        "    elif opcion_menu_general == \"4\":\n",
        "        while True:\n",
        "            print(\"\\n-- Precios/Ventas de Productos --\")\n",
        "            opcion_productos = input(\"Producto chanclas: [1]\\nProducto zapatos: [2]\\nProducto tenis: [3]\\nSalir del menu de productos: [0]\\nIngrese una opcion: \")\n",
        "            if opcion_productos == \"1\":\n",
        "                precio_ventas_primer_semestre = float(input(\"Ingrese el precio de venta del primer semestre: \"))\n",
        "                precio_ventas_segundo_semestre = float(input(\"Ingrese el precio de venta del segundo semestre: \"))\n",
        "                ventas_planeadas_primer_semestre = float(input(\"Ingrese las ventas planeadas del primer semestre: \"))\n",
        "                ventas_planeadas_segundo_semestre = float(input(\"Ingrese las ventas planeadas del segundo semestre: \"))\n",
        "\n",
        "                precio_venta_chanclas = [precio_ventas_primer_semestre, precio_ventas_segundo_semestre, ventas_planeadas_primer_semestre, ventas_planeadas_segundo_semestre]\n",
        "                precio_venta_chanclas_diccionario = {\n",
        "                    \"precio_ventas_primer_semestre\":precio_venta_chanclas[0],\n",
        "                    \"precio_ventas_segundo_semestre\":precio_venta_chanclas[1],\n",
        "                    \"ventas_planeadas_primer_semestre\":precio_venta_chanclas[2],\n",
        "                    \"ventas_planeadas_segundo_semestre\":precio_venta_chanclas[3]\n",
        "                }\n",
        "\n",
        "\n",
        "            elif opcion_productos == \"2\":\n",
        "                precio_ventas_primer_semestre = float(input(\"Ingrese el precio de venta del primer semestre: \"))\n",
        "                precio_ventas_segundo_semestre = float(input(\"Ingrese el precio de venta del segundo semestre: \"))\n",
        "                ventas_planeadas_primer_semestre = float(input(\"Ingrese las ventas planeadas del primer semestre: \"))\n",
        "                ventas_planeadas_segundo_semestre = float(input(\"Ingrese las ventas planeadas del segundo semestre: \"))\n",
        "\n",
        "                precio_venta_zapatos = [precio_ventas_primer_semestre, precio_ventas_segundo_semestre, ventas_planeadas_primer_semestre, ventas_planeadas_segundo_semestre]\n",
        "                precio_venta_zapatos_diccionario = {\n",
        "                    \"precio_ventas_primer_semestre\":precio_venta_zapatos[0],\n",
        "                    \"precio_ventas_segundo_semestre\":precio_venta_zapatos[1],\n",
        "                    \"ventas_planeadas_primer_semestre\":precio_venta_zapatos[2],\n",
        "                    \"ventas_planeadas_segundo_semestre\":precio_venta_zapatos[3]\n",
        "                }\n",
        "\n",
        "            elif opcion_productos == \"3\":\n",
        "                precio_ventas_primer_semestre = float(input(\"Ingrese el precio de venta del primer semestre: \"))\n",
        "                precio_ventas_segundo_semestre = float(input(\"Ingrese el precio de venta del segundo semestre: \"))\n",
        "                ventas_planeadas_primer_semestre = float(input(\"Ingrese las ventas planeadas del primer semestre: \"))\n",
        "                ventas_planeadas_segundo_semestre = float(input(\"Ingrese las ventas planeadas del segundo semestre: \"))\n",
        "\n",
        "                precio_venta_tenis = [precio_ventas_primer_semestre, precio_ventas_segundo_semestre, ventas_planeadas_primer_semestre, ventas_planeadas_segundo_semestre]\n",
        "                precio_venta_tenis_diccionario = {\n",
        "                    \"precio_ventas_primer_semestre\":precio_venta_tenis[0],\n",
        "                    \"precio_ventas_segundo_semestre\":precio_venta_tenis[1],\n",
        "                    \"ventas_planeadas_primer_semestre\":precio_venta_tenis[2],\n",
        "                    \"ventas_planeadas_segundo_semestre\":precio_venta_tenis[3]\n",
        "                }\n",
        "\n",
        "            elif opcion_productos == \"0\":\n",
        "                break\n",
        "\n",
        "    elif opcion_menu_general == \"5\":\n",
        "        while True:\n",
        "            print(\"\\n-- Gasto de administracion y ventas --\")\n",
        "            depreciacion_anual = float(input(\"Ingrese la depreciacion anual: \"))\n",
        "            sueldos_salarios_anuales = float(input(\"Ingrese la cantidad de sueldos y salarios anuales: \"))\n",
        "            comisiones_ventas_proyectadas = float(input(\"Ingrese el porcentaje de comision de las ventas proyectadas: \"))\n",
        "            varios_primer_semestre = float(input(\"Ingrese la cantidad de varios del primer semestre: \"))\n",
        "            varios_segundo_semestre = float(input(\"Ingrese la cantidad de varios del segundo semestre: \"))\n",
        "            intereses_prestamo_anuales = float(input(\"Ingrese la cantidad de intereses por prestamo anuales: \"))\n",
        "\n",
        "            gasto_de_administracion_y_ventas = [depreciacion_anual, sueldos_salarios_anuales, comisiones_ventas_proyectadas, varios_primer_semestre, varios_segundo_semestre, intereses_prestamo_anuales]\n",
        "            gasto_de_administracion_y_ventas_diccionario = {\n",
        "                \"depreciacion_anual\":gasto_de_administracion_y_ventas[0],\n",
        "                \"sueldos_salarios_anuales\":gasto_de_administracion_y_ventas[1],\n",
        "                \"comisiones_ventas_proyectadas\":gasto_de_administracion_y_ventas[2],\n",
        "                \"varios_primer_semestre\":gasto_de_administracion_y_ventas[3],\n",
        "                \"varios_segundo_semestre\":gasto_de_administracion_y_ventas[4],\n",
        "                \"intereses_prestamo_anuales\":gasto_de_administracion_y_ventas[5],\n",
        "            }\n",
        "            break\n",
        "\n",
        "    elif opcion_menu_general == \"6\":\n",
        "        while True:\n",
        "            print(\"\\n-- Gastos de fabricacion indirectos --\")\n",
        "            depreciacion_anual = float(input(\"Ingrese la depreciacion anual: \"))\n",
        "            seguros_anuales = float(input(\"Ingrese la cantidad de seguros anuales: \"))\n",
        "            mantenimiento_primer_semestre = float(input(\"Ingrese la cantidad de mantenimiento de primer semestre: \"))\n",
        "            mantenimiento_segundo_semestre = float(input(\"Ingrese la cantidad de mantenimiento del segundo semestre: \"))\n",
        "            energeticos_primer_semestre = float(input(\"Ingrese la cantidad de energeticos de primer semestre: \"))\n",
        "            energeticos_segundo_semestre = float(input(\"Ingrese la cantidad de energeticos del segundo semestre: \"))\n",
        "            varios_anuales = float(input(\"Ingrese la cantidad de varios anuales: \"))\n",
        "\n",
        "            gastos_fabricacion_indirectos = [depreciacion_anual, seguros_anuales, mantenimiento_primer_semestre, mantenimiento_segundo_semestre, energeticos_primer_semestre, energeticos_segundo_semestre, varios_anuales]\n",
        "            gastos_fabricacion_indirectos_diccionario = {\n",
        "                \"depreciacion_anual\":gastos_fabricacion_indirectos[0],\n",
        "                \"seguros_anuales\":gastos_fabricacion_indirectos[1],\n",
        "                \"mantenimiento_primer_semestre\":gastos_fabricacion_indirectos[2],\n",
        "                \"mantenimiento_segundo_semestre\":gastos_fabricacion_indirectos[3],\n",
        "                \"energeticos_primer_semestre\":gastos_fabricacion_indirectos[4],\n",
        "                \"energeticos_segundo_semestre\":gastos_fabricacion_indirectos[5],\n",
        "                \"varios_anuales\":gastos_fabricacion_indirectos[6],\n",
        "            }\n",
        "            break\n",
        "\n",
        "    elif opcion_menu_general == \"7\":\n",
        "        datos_adicionales()\n",
        "\n",
        "    elif opcion_menu_general == \"8\":\n",
        "        print(\"\\n-- REPORTES --\")\n",
        "        print(\"I. Presupuesto de Operacion.\")\n",
        "        print(\"*\" * 120)\n",
        "        print(\"1. Presupuesto de Ventas\")\n",
        "\n",
        "        importe_venta_chanclas_1 = precio_venta_chanclas_diccionario[\"precio_ventas_primer_semestre\"] * precio_venta_chanclas_diccionario[\"ventas_planeadas_primer_semestre\"]\n",
        "        importe_venta_chanclas_2 = precio_venta_chanclas_diccionario[\"precio_ventas_segundo_semestre\"] * precio_venta_chanclas_diccionario[\"ventas_planeadas_segundo_semestre\"]\n",
        "        importe_venta_chanclas_2016 = importe_venta_chanclas_1 + importe_venta_chanclas_2\n",
        "\n",
        "        importe_ventas_zapatos_1 = precio_venta_zapatos_diccionario[\"precio_ventas_primer_semestre\"] * precio_venta_zapatos_diccionario[\"ventas_planeadas_primer_semestre\"]\n",
        "        importe_ventas_zapatos_2 = precio_venta_zapatos_diccionario[\"precio_ventas_segundo_semestre\"] * precio_venta_zapatos_diccionario[\"ventas_planeadas_segundo_semestre\"]\n",
        "        importe_venta_zapatos_2016 = importe_ventas_zapatos_1 + importe_ventas_zapatos_2\n",
        "\n",
        "        importe_ventas_tenis_1 = precio_venta_tenis_diccionario[\"precio_ventas_primer_semestre\"] * precio_venta_tenis_diccionario[\"ventas_planeadas_primer_semestre\"]\n",
        "        importe_ventas_tenis_2 = precio_venta_tenis_diccionario[\"precio_ventas_segundo_semestre\"] * precio_venta_tenis_diccionario[\"ventas_planeadas_segundo_semestre\"]\n",
        "        importe_ventas_tenis_2016 = importe_ventas_tenis_1 + importe_ventas_tenis_2\n",
        "\n",
        "        importe_venta_1 = precio_venta_tenis_diccionario[\"ventas_planeadas_primer_semestre\"] * precio_venta_tenis_diccionario[\"precio_ventas_primer_semestre\"]\n",
        "        importe_venta_2 = precio_venta_tenis_diccionario[\"ventas_planeadas_segundo_semestre\"] * precio_venta_tenis_diccionario[\"precio_ventas_segundo_semestre\"]\n",
        "        importe_ventas_2016 = importe_venta_1 + importe_venta_2\n",
        "\n",
        "        importe_venta_total_1 = importe_venta_chanclas_1 + importe_ventas_zapatos_1 + importe_ventas_tenis_1\n",
        "        importe_venta_total_2 = importe_venta_chanclas_2 + importe_ventas_zapatos_2 + importe_ventas_tenis_2\n",
        "        importe_ventas_total_2016 = importe_venta_chanclas_2016 + importe_venta_zapatos_2016 + importe_ventas_tenis_2016\n",
        "\n",
        "        presupuesto_de_ventas = [\n",
        "            [\"PRODUCTO Chanclas\", None, None, None],\n",
