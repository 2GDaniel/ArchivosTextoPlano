        "        total_materiales_A_1 = final_total_material_A_requerido_1 + inventario_final_A_1\n",
        "        total_materiales_A_2 = final_total_material_A_requerido_2 + inventario_final_A_2\n",
        "        total_materiales_A_2016 = final_total_material_A_requerido_2016 +inventario_final_A_2016\n",
        "        material_a_comprar_A_1 = total_materiales_A_1 - inventario_final_A_1\n",
        "        material_a_comprar_A_2 = total_materiales_A_2 - inventario_final_A_1\n",
        "        material_a_comprar_A_2016 = total_materiales_A_2016 - inventario_final_A_1\n",
        "        precio_compra_A_1 = costos_primer_semestre_diccionario[\"materia_prima_A_metros\"]\n",
        "        precio_compra_A_2 = costos_segundo_semestre_diccionario[\"materia_prima_A_metros\"]\n",
        "        total_material_pesos_A_1 = material_a_comprar_A_1 * precio_compra_A_1\n",
        "        total_material_pesos_A_2 = material_a_comprar_A_2 * precio_compra_A_2\n",
        "        total_material_pesos_A_2016 = total_material_pesos_A_1 + total_material_pesos_A_2\n",
        "\n",
        "        inventario_final_B_1 = inventario_inicial_primer_semestre_diccionario[\"materia_prima_B_metros\"]\n",
